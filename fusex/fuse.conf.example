    "RequestTimeout" : 30,
    "StreamTimeout" : 60,
    "RedirectLimit" : 3,
    "LogLevel" : "None"
  },
   "recovery" : {
     "read-open" : 1,
     "read-open-noserver" : 1,
     "read-open-noserver-retrywindow" : 15,
     "write-open" : 1,
     "write-open-noserver" : 1,
     "write-open-noserver-retrywindow" : 15
   }
